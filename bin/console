#!/usr/bin/env php
<?php

require_once(__DIR__ . '/../vendor/autoload.php');

set_time_limit(0);

$app = new Symfony\Component\Console\Application();
$app->addCommands([
    new \Myracloud\API\Command\CacheClearCommand(),
    new \Myracloud\API\Command\ErrorPagesCommand(),
    new \Myracloud\API\Command\MaintenanceCommand(),
    new \Myracloud\API\Command\DnsRecordCommand(),
    new \Myracloud\API\Command\ZoneImportCommand(),
]);
$app->run();
